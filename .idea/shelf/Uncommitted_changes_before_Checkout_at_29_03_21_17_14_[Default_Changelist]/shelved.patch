+package controllers
+
+import (
+	"GinAPI/constants"
+	"GinAPI/middlewares"
+	"GinAPI/util"
+	pass "GinAPI/util/password"
+	"github.com/gin-gonic/gin"
+	"net/http"
+)
+
+type PasswordController struct {
+	router *gin.Engine
+}
+
+
+func (pcon *PasswordController) SetRouterPass() (*gin.Engine, error) {
+	r := gin.Default()
+	pcon.router = r
+	pcon.router.Use(middlewares.LoggerToFile())
+	pcon.router.GET("/getpass", pcon.GetPassword)
+	return pcon.router, nil
+}
+func (pcon *PasswordController) GetPassword(c *gin.Context) {
+	//var random pass.Password
+	//randompass, err := pass.GeneratePassword()
+	randompass, err := pass.GeneratePassword()
+	if err != nil {
+		util.HandleFailure(c, http.StatusInternalServerError, constants.FAILED)
+		return
+	}
+	//random.EncodedPassword = randompass
+	util.HandleSuccessWithData(c, randompass)
+
+
+}
